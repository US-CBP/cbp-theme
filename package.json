  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "6.4.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "3.3.6",
    "change-case": "3.0.0",
    "copy-webpack-plugin": "4.0.1",
    "css-loader": "0.22.0",
    "eslint": "3.19.0",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "0.9.1",
