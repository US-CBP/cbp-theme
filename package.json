      "email": "abhinav.pobbati@cbp.dhs.gov"
    }
  ],
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/US-CBP/cbp-theme"
  },
  "engines": {
    "node": ">= 4.2.2"
  },
  "devDependencies": {
    "autoprefixer": "6.4.1",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "6.4.1",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "3.3.6",
    "cbp-rollup-build": "git+https://github.com/yogeshgadge/cbp-rollup-build.git",
    "change-case": "3.0.0",
    "copy-webpack-plugin": "4.0.1",
    "cpx": "1.5.0",
    "css-loader": "0.22.0",
    "eslint": "3.19.0",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "0.9.1",
    "file-loader": "0.8.5",
    "glob": "7.1.2",
    "html-loader": "0.4.3",
    "html-webpack-plugin": "2.22.0",
    "immutable": "3.8.1",
    "jest-cli": "21.2.1",
    "json-loader": "0.5.4",
    "node-sass": "4.5.3",
    "node-sass-magic-importer": "5.1.0",
    "node-sass-tilde-importer": "1.0.1",
    "nouislider": "~8.5.1",
    "npm-run-all": "4.1.2",
    "phantomjs": "2.1.7",
    "pixelmatch": "4.0.2",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "rollup": "0.50.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-cleanup": "1.0.1",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-jst": "1.0.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-sass": "0.6.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1",
    "sass-loader": "4",
    "select2": "4.0.3",
    "selectize": "0.12.1",
    "snazzy": "7.0.0",
    "standard": "10.0.3",
    "strip-loader": "0.1.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webfont-dl": "0.1.2",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.16.1"
  },
  "optionalDependencies": {
    "flag-icon-css": "2.8.0",
    "jquery.inputmask": "3.3.3"
  },
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./kitchensink/dist",
    "lint": "standard 'src/js/**' './kitchensink/kitchensink.js' --fix | snazzy",
    "build:rollup": "rollup -c",
    "build:copy": "cpx \"./node_modules/{roboto-fontface,font-awesome}/fonts/**/*\" ./dist/ &&  cpx \"./dist/**/*\" ./kitchensink/dist",
    "build": "npm-run-all clean build:rollup  build:scss build:copy",
    "build:scss": "node ./build/cbp-rollup-build/sass-builder.js",
    "build:old": "webpack --bail && npm run lint && npm run test",
    "serve": "webpack-dev-server --host=0.0.0.0 --port=8888 --content-base",
    "dev": "npm run clean && npm run build && npm run serve",
    "test": "jest",
    "viz:baseline": "phantomjs e2e/baseline.js",
    "viz:new": "phantomjs e2e/new-viz-diff.js",
    "viz:check": "npm run viz:new && node e2e/viz-diff.js",
    "a11y": "pa11y --standard Section508 --reporter html > a11y.html http://localhost:9000 || true",
    "preversion": "npm test",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "CBP",
    "CF",
    "Common",
    "Frameworks",
    "Team",
    "UI"
  ],
  "files": [
    "dist"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.6",
    "font-awesome": "4.6.3",
    "jquery": "3.3.1",
    "material-design-lite": "1.1.1",
    "mdl-selectfield": "1.0.2",
    "roboto-fontface": "0.8.0"
  }
}
