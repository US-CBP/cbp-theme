    "json-loader": "0.5.4",
    "node-sass": "4.5.3",
    "nouislider": "~8.5.1",
    "postcss-import": "8.1.2",
    "postcss-loader": "0.13.0",
    "sass-loader": "4",
    "select2": "4.0.3",
    "selectize": "0.12.1",
    "snazzy": "7.0.0",
    "standard": "10.0.3",
    "strip-loader": "0.1.0",
    "style-loader": "0.13.0",
    "url-loader": "0.5.7",
    "webfont-dl": "0.1.2",
    "webpack": "1.12.6",
    "webpack-dev-server": "1.16.1"
  },
  "optionalDependencies": {
    "jquery.inputmask": "3.3.3"
  },
  "scripts": {
    "clean": "rm -rf ./dist && rm -rf ./app/kitchensink/dist",
    "lint": "standard 'app/js/**' 'app/kitchensink/kitchensink.js' --fix | snazzy",
    "build": "webpack --bail && npm run test",
    "serve": "webpack-dev-server --host=0.0.0.0 --port=8888 --content-base",
    "dev": "npm run clean && npm run build && npm run serve",
    "test": "jest",
    "a11y": "pa11y --standard Section508 --reporter html > a11y.html http://localhost:9000 || true",
    "preversion": "npm test",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "CBP",
    "CF",
    "Common",
    "Frameworks",
    "Team",
    "UI"
  ],
  "files": [
    "dist"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.6",
    "font-awesome": "4.6.3",
    "material-design-lite": "1.1.1",
    "mdl-selectfield": "1.0.2",
    "roboto-fontface": "0.8.0",
    "yarn": "^1.1.0"
  }
