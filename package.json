{
  "name": "cbp-theme",
  "description": "CBP Common Framework UI Theme",
  "version": "1.8.1",
  "author": "CBP Common Framework",
  "main": "./dist/js/npm",
  "contributors": [
    {
      "name": "Kianoosh Raika",
      "email": "kianoosh.raika@cbp.dhs.gov"
    },
    {
      "name": "Amit Daya",
      "email": "amitdaya@in-di-go.com"
    },
    {
      "name": "David Y. Hodge",
      "email": "david.y.hodge@cbp.dhs.gov"
    },
    {
      "name": "Abhinav Pobbati",
      "email": "abhinav.pobbati@cbp.dhs.gov"
    }
  ],
  "license": "CC0-1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/US-CBP/cbp-theme"
  },
  "engines": {
    "node": ">= 8"
  },
  "devDependencies": {
    "autoprefixer": "8.1.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "bootstrap": "3.3.6",
    "browser-sync": "2.23.6",
    "cssnano": "3.10.0",
    "eslint": "3.19.0",
    "glob": "7.1.2",
    "immutable": "3.8.1",
    "jest-cli": "21.2.1",
    "node-sass": "4.5.3",
    "nouislider": "~8.5.1",
    "npm-run-all": "4.1.2",
    "phantomjs": "2.1.7",
    "pixelmatch": "4.0.2",
    "postcss": "6.0.19",
    "postcss-url": "7.3.1",
    "rollup": "0.50.0",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-cleanup": "1.0.1",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-jst": "1.0.1",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-sass": "0.6.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-uglify": "2.0.1",
    "rollup-watch": "4.3.1",
    "select2": "4.0.3",
    "selectize": "0.12.1",
    "snazzy": "7.0.0",
    "standard": "10.0.3"
  },
  "optionalDependencies": {
    "flag-icon-css": "2.8.0",
    "jquery.inputmask": "3.3.3"
  },
  "scripts": {
    "clean": "rimraf ./dist",
    "lint": "standard 'src/js/**' './kitchensink/kitchensink.js' --fix | snazzy",
    "build": "npm-run-all clean --parallel build:rollup  \"build:scss:postcss -- --production\"",
    "build:scss:postcss": "node ./build/cbp-rollup-build/sass-builder.js",
    "build:rollup": "rollup -c",
    "serve": "browser-sync start --config conf/bs-config.js",
    "dev": "npm-run-all clean --parallel \"build:scss:postcss -- -w\" \"build:rollup -- -w\" serve",
    "test": "jest",
    "viz:baseline": "phantomjs e2e/baseline.js",
    "viz:new": "phantomjs e2e/new-viz-diff.js",
    "viz:check": "npm run viz:new && node e2e/viz-diff.js",
    "a11y": "pa11y --standard Section508  > a11y.html http://localhost:8888 || true",
    "preversion": "npm test",
    "version": "npm run build && git add -A .",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "CBP",
    "CF",
    "Common",
    "Frameworks",
    "Team",
    "UI"
  ],
  "files": [
    "dist"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "bootstrap-sass": "3.3.6",
    "font-awesome": "4.6.3",
    "jquery": "3.3.1",
    "material-design-lite": "1.1.1",
    "mdl-selectfield": "1.0.2",
    "roboto-fontface": "0.8.0"
  },
  "peerDependencies": {
    "chokidar" : "*"
  }
}
